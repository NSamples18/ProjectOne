<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="396.0" prefWidth="436.0"
            xmlns="http://javafx.com/javafx/23.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="edu.westga.cs3211.LandingPage.view.LandingPageCodeBehind">

    <children>

        <Label fx:id="greetingLabel"
               layoutX="100.0" layoutY="68.0"
               prefHeight="17.0" prefWidth="212.0"
               text="Hello, ">
            <font><Font size="18.0"/></font>
        </Label>

        <Button fx:id="addStockButton"
        layoutX="183.0"
        layoutY="106.0"
        onAction="#handleAddStock"
        text="Add Stock" />

		<Button fx:id="viewStockButton"
        layoutX="181.0"
        layoutY="145.0"
        mnemonicParsing="false"
        onAction="#handleViewStock"
        text="View Stock" />

		<Button fx:id="logoutButton"
        layoutX="351.0"
        layoutY="14.0"
        mnemonicParsing="false"
        onAction="#handleLogout"
        text="Log Out" />


    </children>

</AnchorPane>
